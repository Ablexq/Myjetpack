<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.mycardview">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".activitylifecyclecallbacks.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".activitylifecyclecallbacks.MainActivity"/>
        <activity android:name=".cardview.CardViewActivity"/>
        <activity android:name=".lifecycle.MyActivity1"/>
        <activity android:name=".livedataviewmodel.MyActivity2"/>
        <activity android:name=".databinding.view.DemoActivity1" />
        <activity android:name=".databinding.view.DemoActivity2" />
        <activity android:name=".databinding.view.DemoActivity3" />

        <activity android:name=".IndexActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".databinding2.view.TestActivity1" />
        <activity android:name=".databinding2.view.TestActivity2" />
        <activity android:name=".databinding2.view.TestActivity3" />
        <activity android:name=".databinding3.view.My1Activity" />
        <activity android:name=".databinding3.view.My2Activity" />
        <activity android:name=".databinding3.view.My3Activity" />
    </application>

</manifest>